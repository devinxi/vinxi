{
  "name": "@vinxi/vite-preset-solid",
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "build:cjs": "esbuild src/index.ts --format=cjs --platform=node --bundle --external:vite-plugin-solid --external:vite-plugin-inspect --external:@rollup/plugin-babel --external:babel-plugin-solid-labels --external:vite-tsconfig-paths > dist/index.js",
    "build:esm": "esbuild src/index.ts --format=esm --platform=node --bundle --external:vite-plugin-solid --external:vite-plugin-inspect --external:@rollup/plugin-babel --external:babel-plugin-solid-labels --external:vite-tsconfig-paths > dist/index.mjs",
    "build": "mkdir -p dist && npm run build:cjs && npm run build:esm",
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf node_modules"
  },
  "dependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "vite-plugin-inspect": "*",
    "babel-plugin-solid-labels": "^0.9.4",
    "@vinxi/vite-plugin-solid": "*",
    "vite-tsconfig-paths": "*"
  },
  "devDependencies": {
    "esbuild": "^0.14.8"
  }
}
