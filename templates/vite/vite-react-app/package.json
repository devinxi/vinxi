  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "@vinxi/scripts": "*",
    "@vinxi/tsconfig": "*",
    "typescript": "^4.5.3",
    "vite": "^2.6.14"
  }
}
